<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.duoduolicai360.simpleone">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".demo1.Activity1"/>
        <activity android:name=".demo2.Activity2"/>
        <activity android:name=".demo3.Activity3"/>
        <activity android:name=".demo4.Activity4"/>
        <activity android:name=".demo5.Activity5"/>
        <activity android:name=".demo6.Activity6"
            android:theme="@style/AppTheme.NoActionBar"/>
    </application>

</manifest>